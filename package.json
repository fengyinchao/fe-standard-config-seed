{
  "name": "@vigor/eslint-config-fed",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:fengyinchao/fe-standard-config-seed.git"
  },
  "author": "Feng Yinchao",
  "license": "MIT",
  "files": [
    "eslintrc.javascript-base.js",
    "eslintrc.node.js",
    "eslintrc.react.js",
    "eslintrc.vue.js",
    "eslintrc.rn.js",
    "eslintrc.typescript-base.js",
    "js-ts-common.rule.js",
    ".editorconfig",
    ".eslintignore",
    ".gitignore",
    ".npmrc",
    ".prettierrc.js",
    ".prettierignore",
    "commitlint.config.js"
  ],
  "exports": {
    "./javascript-base": {
      "require": "./eslintrc.javascript-base.js",
      "default": "./eslintrc.javascript-base.js"
    },
    "./node": {
      "require": "./eslintrc.node.js",
      "default": "./eslintrc.node.js"
    },
    "./react": {
      "require": "./eslintrc.react.js",
      "default": "./eslintrc.react.js"
    },
    "./vue": {
      "require": "./eslintrc.vue.js",
      "default": "./eslintrc.vue.js"
    },
    "./rn": {
      "require": "./eslintrc.rn.js",
      "default": "./eslintrc.rn.js"
    },
    "./typescript-base": {
      "require": "./eslintrc.typescript-base.js",
      "default": "./eslintrc.typescript-base.js"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint",
      "prettier --write"
    ],
    "*.{css,less,scss}": [
      "prettier --write"
    ],
    "*.{html,md,json,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@commitlint/cli": "9.1.2",
    "@commitlint/config-conventional": "12.1.4",
    "@vigor/eslint-plugin-fed": "1.0.5",
    "@typescript-eslint/eslint-plugin": "4.7.0",
    "@typescript-eslint/parser": "4.7.0",
    "babel-eslint": "10.1.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-plugin-deprecation": "1.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "33.0.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "husky": "4.3.0",
    "lint-staged": "10.4.0",
    "prettier": "2.1.2",
    "typescript": "4.1.6"
  }
}
